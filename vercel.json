{
  "trailingSlash": true,
  "redirects": [
    {
      "source": "/:path+",
      "has": [
        {
          "type": "host",
          "value": "socalimmigrationservices.com"
        }
      ],
      "destination": "https://www.socalimmigrationservices.com/:path+/",
      "permanent": true
    },
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "socalimmigrationservices.com"
        }
      ],
      "destination": "https://www.socalimmigrationservices.com/",
      "permanent": true
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/blog/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
